{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "rootDir": "src",
    "outDir": "./dist/undefined",
    "noEmit": true
  },
  "include": [
    "./src"
  ],
  "typedocOptions": {
    "inputFiles": [
      "./src/index.ts"
    ],
    "exclude": [
      "**/*.spec.ts",
      "**/spec/*.ts"
    ],
    "mode": "file",
    "out": "../../docs/charts-react/api",
    "readme": "none",
    "theme": "gitbook",
    "excludeNotExported": true,
    "excludePrivate": true,
    "hideGenerator": true,
    "hideSources": true
  }
}